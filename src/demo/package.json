  "dependencies": {
    "audiomotion-analyzer": "^4.3.0",
    "butterchurn": "^2.6.7",
    "butterchurn-presets": "^2.4.7",
    "icecast-metadata-player": "file:../icecast-metadata-player",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "@minify-html/node": "^0.15.0",
    "gh-pages": "^6.1.0",
    "prettier": "^3.1.0"
